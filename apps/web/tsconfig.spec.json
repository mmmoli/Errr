{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/vitest",
    "types": [
      "vitest/globals",
      "vitest/importMeta",
      "vite/client",
      "node",
      "vitest",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ],
    "jsx": "react-jsx",
    "rootDirs": [".", "./.react-router/types"]
  },
  "include": [
    "vite.config.ts",
    "vite.config.mts",
    "vitest.config.ts",
    "vitest.config.mts",
    "app/**/*.test.ts",
    "app/**/*.spec.ts",
    "app/**/*.test.tsx",
    "app/**/*.spec.tsx",
    "app/**/*.test.js",
    "app/**/*.spec.js",
    "app/**/*.test.jsx",
    "app/**/*.spec.jsx",
    "app/**/*.d.ts",
    "./.react-router/types/**/*.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.app.json"
    }
  ]
}
